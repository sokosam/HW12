# Since the ".env" file is gitignored, you can use the ".env.example" file to
# build a new ".env" file when you clone the repo. Keep this file up-to-date
# when you add new variables to `.env`.

# This file will be committed to version control, so make sure not to have any
# secrets in it. If you are cloning this repo, create a copy of this file named
# ".env" and populate it with your secrets.

# When adding additional environment variables, the schema in "/src/env.js"
# should be updated accordingly.

# big garbage url no need
# POSTGRES_URL="postgresql://postgres:password@localhost:5432/HW12"
# Recommended for most uses
POSTGRES_URL=secret

# For uses requiring a connection without pgbouncer
POSTGRES_URL_UNPOOLED=secret

# Parameters for constructing your own connection string
PGHOST=secret
PGHOST_UNPOOLED=secret
PGUSER=secret
PGDATABASE=secret
PGPASSWORD=secret
# Parameters for Vercel Postgres Templates
POSTGRES_URL=secret
POSTGRES_URL_NON_POOLING=secret
POSTGRES_USER=secret
POSTGRES_HOST=secret
POSTGRES_PASSWORD=secret
POSTGRES_DATABASE=secret
POSTGRES_URL_NO_SSL=secret
POSTGRES_PRISMA_URL=secret
# Neon Auth environment variables for Next.js
NEXT_PUBLIC_STACK_PROJECT_ID=secret
NEXT_PUBLIC_STACK_PUBLISHABLE_CLIENT_KEY=secret
STACK_SECRET_SERVER_KEY=secret

# ElevenLabs API
ELEVENLABS_API_KEY=your_elevenlabs_api_key_here

# Twilio Configuration
TWILIO_ACCOUNT_SID=your_twilio_account_sid_here
TWILIO_AUTH_TOKEN=your_twilio_auth_token_here
TWILIO_PHONE_NUMBER=+1234567890
ALERT_PHONE_NUMBER=+1234567890